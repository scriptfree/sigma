local scripts = {
    ["MVSD"] = "https://raw.githubusercontent.com/scriptfree/sigma/main/kai-cenat";
    ["ZR"] = "https://raw.githubusercontent.com/scriptfree/lua/main/private/zombie-rush";
}
local bindableFunction = Instance.new("BindableFunction")

function bindableFunction.OnInvoke(choice)
    if scripts[choice] then
        loadstring(game:HttpGet(scripts[choice]))()
    end
end


game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "HEY!";
    Text = "Choose a script";
    Button1 = "MVSD";
    Button2 = "ZR";
    Duration = 60000;
    Callback = bindableFunction;
})
